function FnConn() {

    let url = "";
    let username = "";
    let password = "";


    //get and set password
    this.username = "admin";
    this.password = "admin";
    this.url = "http://natsapi.altair.davecutting.uk/jsonapi.php/login";

}


FnConn.prototype.connect = function() {


    let url = this.url;
    let usr = this.username;
    let pass = this.password;
    url = url + "?naun=" + usr + "&napw=" + pass;

    $("#result").html(url);

    /*   $.get("http://httpbin.org/get",function(data){
        $("#result").html(data);
      }
      ); */


    $.get("http://httpbin.org/get",function(data,status) {
        $("#result").html(data);
        //$("#error").html(status);
    }, "json");






    //$("#result").html("fail");
}

let a = new FnConn();
a.connect();